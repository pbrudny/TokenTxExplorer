{
  "name": "tokentxexplorer",
  "version": "1.0.0",
  "main": "dist/index.mjs",
  "scripts": {
    "build": "tsc && npm run rename",
    "rename": "find dist -name '*.js' -exec sh -c 'mv \"$0\" \"${0%.js}.mjs\"' {} \\;",
    "export": "pkg .",
    "start": "node dist/index.mjs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Piotr Brudny",
  "license": "MIT",
  "description": "Token transaction explorer",
  "type": "commonjs",
  "bin": "dist/index.mjs",
  "dependencies": {
    "axios": "^1.7.2",
    "chalk": "^5.3.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.5",
    "web3": "^4.11.1"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^22.1.0",
    "typescript": "^5.5.4"
  },
  "pkg": {
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-win-x64"
    ]
  }
}
